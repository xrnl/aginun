{
  "name": "aginun",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "vue": "vue-cli-service",
    "i18n:report": "vue-cli-service i18n:report --src 'src/**/*.?(js|ts|vue)' --locales 'src/i18n/messages/**/*.json'",
    "i18n:extract": "vue-i18n-extract-translations -v 'src/**/*.?(js|ts|vue)' -l 'src/i18n/messages/' --target 'nl' --fill '' --key 'i18n'",
    "prei18n:push": "npm run i18n:extract",
    "i18n:push": "node src/i18n/scripts/push-translations.js",
    "prei18n:pull": "node src/i18n/scripts/delete-translations.js",
    "i18n:pull": "node src/i18n/scripts/pull-translations.js",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:dev": "vue-cli-service test:unit --watchAll",
    "test:e2e": "vue-cli-service test:e2e",
    "lint": "vue-cli-service lint --no-fix",
    "start": "vue-cli-service serve"
  },
  "dependencies": {
    "@mdi/font": "^4.5.95",
    "@saeris/vue-spinners": "^1.0.8",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "axios": "^0.21.0",
    "core-js": "^3.7.0",
    "graphql": "^14.7.0",
    "graphql-tag": "^2.11.0",
    "lodash": "^4.17.20",
    "qs": "^6.9.4",
    "vee-validate": "^3.4.5",
    "vue": "^2.6.12",
    "vue-apollo": "^3.0.5",
    "vue-axios": "^3.2.0",
    "vue-i18n": "^8.17.3",
    "vue-i18n-extract-translations": "^0.1.3",
    "vue-infinite-loading": "^2.4.5",
    "vue-router": "^3.4.9",
    "vuetify": "^2.1.0",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^1.0.0",
    "@types/jest": "^24.9.1",
    "@types/lodash": "^4.14.165",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-e2e-cypress": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "dotenv": "^8.2.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "node-sass": "^5.0.0",
    "prettier": "^1.19.1",
    "sass": "^1.29.0",
    "sass-loader": "^8.0.2",
    "style-resources-loader": "^1.3.3",
    "typescript": "~3.9.3",
    "unfetch": "^4.2.0",
    "vue-cli-plugin-i18n": "~1.0.1",
    "vue-cli-plugin-style-resources-loader": "^0.1.4",
    "vue-cli-plugin-vuetify": "^1.1.1",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.6.0"
  }
}
